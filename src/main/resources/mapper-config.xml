<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
 <configuration>
	<settings>
		<setting name="cacheEnabled" value="true" />
		<setting name="useGeneratedKeys" value="false" />
		<setting name="defaultExecutorType" value="REUSE" />
		<setting name="defaultStatementTimeout" value="60" />
		<setting name="mapUnderscoreToCamelCase" value="false" />
	</settings>

	<typeAliases>
		<typeAlias alias="ProductDetailDTO"
			type="com.theharmm.domain.ProductDetailDTO" />
		<typeAlias alias="ProductSizeDTO"
			type="com.theharmm.domain.ProductSizeDTO" />
		<typeAlias alias="MemberAddressDTO"
			type="com.theharmm.domain.MemberAddressDTO" />
		<typeAlias alias="AccountDTO"
			type="com.theharmm.domain.AccountDTO" />
		<typeAlias alias="SellDTO"
			type="com.theharmm.domain.SellDTO" />
		<typeAlias alias="BuyDTO"
			type="com.theharmm.domain.BuyDTO" />
		<typeAlias alias="ProductDTO"
			type="com.theharmm.domain.ProductDTO" />	
		<typeAlias alias="KeywordDTO"
			type="com.theharmm.domain.KeywordDTO" />
		<!-- ShowLive -->
		<typeAlias alias="ShowLiveChatDTO"
			type="com.theharmm.domain.ShowLiveChatDTO" />
		<typeAlias alias="ShowLiveChannelDTO"
			type="com.theharmm.domain.ShowLiveChannelDTO" />
		<typeAlias alias="ShowLiveBiddingDTO"
			type="com.theharmm.domain.ShowLiveBiddingDTO" />
		<typeAlias alias="ShowLiveAuctionFinalPersonDTO"
			type="com.theharmm.domain.ShowLiveAuctionFinalPersonDTO" />
		<!-- AdminChatting -->
		<typeAlias alias="ChatRoomDTO"
			type="com.theharmm.domain.ChatRoomDTO" />
		<typeAlias alias="ChatMessageDTO"
			type="com.theharmm.domain.ChatMessageDTO" />
		<!-- BrandChatting -->
		<typeAlias alias="BrandChatRoomDTO"
			type="com.theharmm.domain.BrandChatRoomDTO" />
		<typeAlias alias="BrandChatMessageDTO"
			type="com.theharmm.domain.BrandChatMessageDTO" />
	</typeAliases>
</configuration>